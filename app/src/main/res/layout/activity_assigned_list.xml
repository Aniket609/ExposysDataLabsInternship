<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/intern_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="                   "
        android:textAppearance="?android:attr/textAppearanceLarge"
        app:layout_constraintStart_toStartOf="@+id/applicant_list"
        app:layout_constraintTop_toTopOf="@+id/applicant_list" />

    <TextView
        android:id="@+id/intern_domain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="                   "
        android:textAppearance="?android:attr/textAppearanceLarge"
        app:layout_constraintStart_toStartOf="@+id/applicant_list"
        app:layout_constraintTop_toBottomOf="@+id/intern_duration" />

    <TextView
        android:id="@+id/intern_duration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="                   "
        android:textAppearance="?android:attr/textAppearanceLarge"
        app:layout_constraintStart_toStartOf="@+id/applicant_list"
        app:layout_constraintTop_toBottomOf="@+id/intern_name" />

    <TextView
        android:id="@+id/intern_paymentid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="                   "
        android:textAppearance="?android:attr/textAppearanceLarge"
